// Zed settings
//
// For information on how to configure Zed, see the Zed
// documentation: https://zed.dev/docs/configuring-zed
//
// To see all of Zed's default settings without changing your
// custom settings, run `zed: open default settings` from the
// command palette (cmd-shift-p / ctrl-shift-p)
{
  "icon_theme": "Zed (Default)",
  "show_edit_predictions": false,
  "helix_mode": false,
  "agent": {
    "default_model": {
      "provider": "google",
      "model": "gemini-2.5-flash"
    },
    "model_parameters": []
  },
  "edit_predictions": {
    "mode": "subtle"
  },
  "agent_buffer_font_size": 16.0,
  "telemetry": {
    "diagnostics": true,
    "metrics": false
  },
  "base_keymap": "JetBrains",
  "ui_font_family": "Adwaita Sans",
  "ui_font_weight": 500,
  "ui_font_size": 17,
  "buffer_font_family": "Maple Mono",
  "buffer_font_fallbacks": ["Liberation Mono"],
  "buffer_font_size": 17,
  "buffer_font_features": {
    "calt": true
  },
  "terminal": {
    "font_family": "Maple Mono NF",
    "font_fallbacks": ["Liberation Mono"]
  },
  "buffer_line_height": {
    "custom": 1.4
  },
  "theme": {
    "mode": "system",
    "light": "Zedokai Light (Filter Sun)",
    "dark": "Zedokai (Filter Octagon)"
  },
  "auto_update": false,
  "file_types": {
    "HTML": ["gohtml"]
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "disable_ai": false,
  "preferred_line_length": 120,
  "soft_wrap": "bounded",
  "show_wrap_guides": false
}
